        <img class="card-img-top" src=<%= arr[i].imgLink %> alt=<%= arr[i].movieName %> />
        <div class="card-body">
          <p class="card-text"><%= arr[i].movieName %></p>
          <a href="#" class="btn btn-primary">Watch Now</a>
        </div>
      </div>
      <% } %>

    </section>
</div>








